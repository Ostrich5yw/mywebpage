<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.example.mywebpage.dao.StuDao">

    <select id="getInfo" parameterType="String" resultType="com.example.mywebpage.bean.StuBean">
        SELECT * FROM student WHERE user_name = #{user_name} AND user_password = #{user_password}
    </select>
    <select id="getAll" resultType="com.example.mywebpage.bean.StuBean">
        SELECT * FROM student
    </select>
</mapper>